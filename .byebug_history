exit
sign_out
if signed_out
      render json: { status: { code: 200, message: 'Logged out successfully.' } }, status: :ok
    else
      render json: { status: { code: 401, message: 'Could not log out.' } }, status: :unauthorized
    end
signed_out
signed_out = Devise.sign_out_all_scopes
current_user
current_user = warden.user
warden.user
current_user
puts "devise_jwt_secret_key: #{Rails.application.credentials.devise_jwt_secret_key}
exit
